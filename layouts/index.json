{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "Organization",
      "name": "{{ .Site.Title }}",
      "url": "{{ .Site.BaseURL }}",
      "logo": "{{ .Site.BaseURL }}imagens/logo.png",
      "award": "{{ .Site.Params.premio }}",
      "slogan": "{{ .Site.Params.slogan }}"
    },
    {
      "@type": "Person",
      "name": "{{ .Site.Params.author }}",
      "jobTitle": "Chef de Criação",
      "knowsAbout": ["Culinaria Criativa",Sem Desperdicio na Cozinha", "Gastronomia Desidratada", "Psicologia", "Nutrição"]
    },
    {
      "@type": "OfferCatalog",
      "name": "Catálogo ChefBox",
      "itemListElement": [
        {{ range $index, $element := (where .Site.RegularPages "Section" "receitas") }}
        {{ if $index }},{{ end }}
        {
          "@type": "Product",
          "name": "{{ .Title }}",
          "sku": "{{ .Params.sku }}",
          "offers": {
            "@type": "Offer",
            "price": "132.00",
            "priceCurrency": "BRL"
          }
        }
        {{ end }}
      ]
    }
  ]
}
